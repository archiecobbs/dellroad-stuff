<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,CT_CONSTRUCTOR_THROW"/>
    </Match>
    <Match>
        <Class name="org.dellroad.stuff.vaadin.fieldbuilder.FieldBuilderGeneratorMojo"/>
        <Method name="execute"/>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_BAD_PRACTICE"/>
    </Match>
</FindBugsFilter>
